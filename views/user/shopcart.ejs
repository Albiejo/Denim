                method: 'POST',
                data: JSON.stringify({ userId, cartId, productId, count }),
                contentType: 'application/json',
                success: (response) => {
                    $('.qty-val[data-item-id="' + cartId + '"]').text(response.updatedQuantity),
                        $('.subtotal-val[data-item-id="' + cartId + '"]').text(response.productValue),
                        $('.totalValue[data-item-id="totalValue"]').text(response.cartTotal)
                    if (response.message) {
                        Swal.fire({
                            icon: 'error',
                            title: 'Oops...',
                            text: response.message,
                        });
                    }
                }
            })
        }


