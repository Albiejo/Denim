        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                            <%if(productsInCart.length<1){%>
                                <h5 style="color: red;">No products in cart yet !</h5>
                                <%}else{%>

                                    <div id="errormsg" style="display: none;">
                                        <!-- SweetAlert will be displayed here -->
                                    </div>
                                    <a style="color:red;font-weight: bold;" href="/clearCart"><button class="btn btn-danger">Clear cart</button></a>
                                    <a class="btn " href="/loginhomepage"><i class="fi-rs-shopping-bag mr-10"></i>Continue
                                        Shopping</a>  
                                    <table class="table shopping-summery text-center clean">
                                        <thead>
                                            <tr class="main-heading">
                                                <th scope="col">Image</th>
                                                <th scope="col">Name</th>
                                                <th scope="col">Size</th>
                                                <th scope="col">Price</th>
                                                <th scope="col">Quantity</th>
                                                <th scope="col">Sub total</th>
                                                <th scope="col">Remove</th>


                                            </tr>
                                        </thead>
                                        <tbody>

                                            <%for(let i=0; i< productsInCart.length; i++){%>
                                                <tr>
                                                    <td class="image product-thumbnail">
                                                        <a href="/productview?id=<%=productsInCart[i]._id%>"><img
                                                                src="/cloud_images/<%=productsInCart[i].images[0].url%>"
                                                                alt="#"></a>
                                                    </td>
                                                    <td class="product-des product-name">
                                                        <h5 class="product-name"><a
                                                                href="/productview?id=<%=productsInCart[i]._id%>">
                                                                <%=productsInCart[i].productName%>
                                                            </a></h5>
                                                        <p class="font-xs">
                                                            <%=productsInCart[i].Discription%>
                                                        </p>
                                                    </td>

                                                    <td>
                                                        <span>
                                                            <%=userData.cart[i].size%>
                                                        </span>
                                                    </td>

                                                    <td class="price" data-title="Price"><span>₹
                                                            <%=productsInCart[i].offerPrice%>
                                                        </span>
                                                    </td>

                                                    <td class="text-center" data-title="Stock">


                                    <div class="detail-qty border radius m-auto">
                                    <a class="qty-up" data-item-id="<%= userData.cart[i]._id %>" data-item-index="<%= i %>" onclick="updateQuantity('<%=userData._id%>','<%= userData.cart[i]._id %>','<%=productsInCart[i]._id%>',1)"><i class="fi-rs-angle-small-up"></i></a>


                                                            <span class="qty-val"
                                                                data-item-id="<%= userData.cart[i]._id %>">
                                                                <%= userData.cart[i].quantity %>
                                                            </span>

                                                            <a class="qty-down"
                                                                data-item-id="<%= userData.cart[i]._id %>"
                                                                data-item-index="<%= i %>"
                                                                onclick="updateQuantity('<%=userData._id%>','<%= userData.cart[i]._id %>','<%=productsInCart[i]._id%>',-1)"><i
                                                                    class="fi-rs-angle-small-down"></i></a>
                                                        </div>
                                                    </td>
 
                                                    <td class="text-center">
                                                        <strong class="subtotal-val"
                                                            data-item-id="<%= userData.cart[i]._id%>">₹
                                                            <%=userData.cart[i]. subtotal%>
                                                        </strong>
                                                    </td>

                                                    <td class="action" data-title="Remove">
                                                        <form action="/deletecartitem?itemId=<%=userData.cart[i]._id%>" method="post" style="display: inline;">
                                                            <button type="button" class="btn btn-sm font-sm btn-light rounded" onclick="confirmDelete(this.form)" ><i class="fi-rs-trash"></i></button>
                                                        </form>
                                                        
                                                    </td>
                                                   

                                                </tr>
                                                <%}%>



                                        </tbody>
                                    </table>
                                    <%}%>
                        </div>
                        <div class="divider center_icon mt-50 mb-50"><i class="fi-rs-fingerprint"></i></div>
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="cart-action text-start">
                                    <!-- <a class="btn  mr-10 mb-sm-15"><i class="fi-rs-shuffle mr-10"></i>Update Cart</a> -->
                                      
                                    <a href="/usercheckout" class="btn" style="display: inline-block;margin-left: 2%; "> <i class="fi-rs-box-alt mr-10"></i> Proceed To
                                            CheckOut</a>     
                                </div>
                            </div>
                                <div class="col-lg-6 col-md-12">
                                   
                                    <div class="border p-md-4 p-30 border-radius cart-totals">
                                      
                                        <div class="heading_s1 mb-3">
                                            <h4>Cart Totals</h4>
                                        </div>
                                        <div class="table-responsive">
                                            <table class="table">
                                                <tbody>
    
                                                    <tr>
                                                        <td class="cart_total_label ">Cart Subtotal</td>
                                                        <td class="cart_total_amount">
                                                            <span class="font-lg fw-900 text-brand totalValue"
                                                                id="cartTotalAmount" data-item-id="totalValue">₹
                                                                <%=userData.CartTotal%>
                                                            </span>
                                                        </td>
                                                    </tr>
        
        
                                                    <tr>
                                                        <td class="cart_total_label">Shipping</td>
                                                        <td class="cart_total_amount"> <i class="ti-gift mr-5"></i> Free
                                                            Shipping</td>
                                                    </tr>
        
        
                                                    <tr>
                                                        <td class="cart_total_label">Total</td>
                                                        <td class="cart_total_amount"><strong><span
                                                                    class="font-xl fw-900 text-brand totalValue"
                                                                    id="cartTotalAmounts" data-item-id="totalValue">₹
                                                                    <%=userData.CartTotal%>
                                                                </span></strong>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                        </div>
                        <div id="cartData" data-products='<%= JSON.stringify(productsInCart) %>'></div>
                        
                    </div>
                </div>
            </div>
            </div>
        </section>
