    <script>

document.addEventListener("DOMContentLoaded", () => {
  const cancelOrderButton = document.getElementById("cancelOrderButton");
  cancelOrderButton.addEventListener("click", (event) => {
    event.preventDefault(); 
    const orderUrl = cancelOrderButton.getAttribute("data-order-url");
    Swal.fire({
      title: 'Are you sure?',
      text: 'Do you want to cancel this order?',
      icon: 'warning',
      showCancelButton: true,
      confirmButtonText: 'Yes, cancel it!',
      cancelButtonText: 'No, keep it',
    }).then((result) => {
      if (result.isConfirmed) {
        // User confirmed the cancellation, and you have the order URL
        // Now you can handle the cancellation logic here
        window.location.href = orderUrl; // Redirect to the order URL for cancellation
      }
    });
  });
});




//return modal
$('#requestReturnButton').on('click', function() {
    $('#returnReasonModall').modal('show');
});

$('#confirmReturns').on('click', function() {
    $('#returnReasonModall').modal('hide');
    var selectedReason = document.getElementById('returnReasons').value
    $.ajax({
        type: "POST",
        url: "/requestReturn/<%=Orderr._id%>/ ",
        data:{selectedReason:selectedReason },
        success: function (response) { 
        window.location.href = '/fullOrderDetails?orderid=<%= Orderr._id %>'
        }
        
    })
});



function showmodal(orderid, itemid) {

    $('#returnReasonModal2').modal('show');

    $('#confirmReturnbutton2').on('click', function() {
        $('#returnReasonModal2').modal('hide');
        var selectedReason = document.getElementById('returnReason').value;

        // Use the orderid and itemid variables to construct the URL
        var url = "/returnitem/" + orderid + "/" + itemid;

        $.ajax({
            type: "POST",
            url: url, // Use the constructed URL
            data: { selectedReason: selectedReason },
            success: function(response) {
                window.location.href = '/fullOrderDetails?orderid=' + orderid;
            }
        });
    });
}


</script>



