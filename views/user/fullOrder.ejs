                                                <div class="container-fluid image product-thumbnail" style="width: 130px; overflow: hidden;">
                                                <img src="/cloud_images/<%= productData[i].images[0].url %>" class="img-fluid" style="max-width: 100%; max-height: 130px;" alt="Product 1">
                                                </div>
                                            </td>

                                            <td>
                                             <%= productData[i].productName %>
                                            </td>
                                            <td>
                                                x<%= items[i].quantity %>
                                            </td>
                                          
                                            <td>
                                           
                                            ₹ <%= items[i].subtotal %>

                                            </td>

                                            <td>
                                                <%if(items[i].status==="Pending"){%>
                                                    <p class="card-text" style="color: yellow;font-weight: bold;"><%=items[i].status%></p>
                                                    <%}else if(items[i].status==="Cancelled"){%>
                                                        <p class="card-text" style="color: rgb(255, 55, 0);font-weight: bold;"><%=items[i].status%></p>
                                                    <%}else if(items[i].status==="Delivered"){%>
                                                        <p class="card-text" style="color: rgb(1, 255, 1); font-weight: bold;"><%=items[i].status%></p>
                                                    <%} else {%>
                                                        <p class="card-text" style="color: blue; font-weight: bold;"><%=items[i].status%></p>
                                                    <%}%>
                                            </td>
                                            <td>
                                                <% if (items.length < 2) { %>
                                                    <!-- no cancel button since item is less than 2 -->
                                                <% } else { %>


                                                    <% if (items[i].status === "Delivered") { %>
                                                        <%if(Orderr.returnStatus==="Request Pending" || Orderr.returnStatus==="Request Accepted" ){%>
                                                            <button disabled>Request Return</button>
                                                        <%} else { %>

                                                            <%if(items[i].returnStatus==="Request Pending" || items[i].returnStatus==="Request Accepted"){%>
                                                                <button disabled>Request Return</button>
                                                                <span style="color: red;font-weight: bold;">Return <%=items[i].returnStatus%></span>

                                                            <% } else {%>
                                                            <button onclick="showmodal('<%= Orderr._id %>', '<%= items[i]._id %>')" class="">Request Return</button>
                                                            <% } %>

                                                    <% } %>
                


                                                    <% } else if (items[i].status === "Cancelled") { %>
                                                        <button class="disabled" disabled>Cancel Order</button>
                                                    <% } else { %>
                                                        <a href="/cancelItem/<%= Orderr._id %>/<%= productData[i]._id %>">
                                                            <button class="btn-danger" data-item-url="/cancelItem/<%= Orderr._id %>/<%= productData[i]._id %>" id="individualCancel">Cancel Item</button>
                                                        </a>
                                                    <% } %>
                                                <% } %>
                                                
                                            </td>
                                        </tr>
                                        <%}%>
                                </tbody>
                            </table>

                            <!--return reason modal -->
                           
                     <!--return reason modal -->
                        </div>
