        <div class="card">
            <div class="card-header">
                <h5>Edit your address</h5>
            </div>
            <div class="card-body">
                <form class="form" method="post" action="/updateuseraddress">
                    <%if (typeof message !=='undefined' ) {%>
                        <p style="color:red;">
                            <%=locals.message %>
                        </p>
                        <%}%>

                    <div class="row">
                        <div class="form-group col-md-12">
                            <label>name<span class="required">*</span></label>
                            <input required class="form-control square" name="name" type="text" value="<%=addressData.Name%>">

                            <label>House name<span class="required">*</span></label>
                            <input required class="form-control square" name="House-name" type="text"  value="<%=addressData.Housename%>">

                            <label>Street<span class="required">*</span></label>
                            <input required class="form-control square" name="Street-name" type="text"  value="<%=addressData.Street%>">

                            <label>Pincode<span class="required">*</span></label>
                            <input required class="form-control square" name="Pincode" type="number"  value="<%=addressData.Pincode%>">

                    
                            <label>Country<span class="required">*</span></label>                          
                            <input required class="form-control square"name="Country" type="text"  value="<%=addressData.Country%>">

                            <label>Phone<span class="required">*</span></label>
                            <input required class="form-control square" name="Phone" type="number"  value="<%=addressData.Phone%>">

                            <input type="hidden" name="addressId" value="<%=addressData._id%>">
                        </div>

                        <div class="col-md-12">
                            <button type="submit" class="btn btn-fill-out submit" name="submit" value="Submit">Update</button>
                        </div>
                    </div>
                </form>
