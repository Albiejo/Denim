<%-include(".././../Layouts/adminhead.ejs")%>
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Order Management</h2>
                </div>
            </div>
            <div class="card mb-4">
               
               
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>#ID</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Order Value</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Ordered on</th>
                                    <th scope="col" class="text-end"> Action </th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                <%for(let i =0; i< orderData.length; i++){%>
                                   
                                <tr >
                                   
                                    <td ><%=orderData[i].orderId%></td>
                                    <td><b><%=orderData[i].customerId.name%></b></td>
                                    <td><span><%=orderData[i].customerId.email%></span></td>
                                    <td>â‚¹ <%=orderData[i].totalAmount%></td>
                                    <%if(orderData[i].status==="Delivered"){%>
                                    <td><span class="badge rounded-pill alert-warning" style="color: rgb(0, 1, 0);"><%=orderData[i].status%></span></td>
                                    <%}else if(orderData[i].status==="Pending"){%>
                                        <td><span class="badge rounded-pill alert-warning" style="color: rgb(0, 21, 255);"><%=orderData[i].status%></span></td>
                                    <%} else if(orderData[i].status==="Shipped") {%>
                                        <td><span class="badge rounded-pill alert-warning" style="color: rgb(0, 0, 0);"><%=orderData[i].status%></span></td>
                                    <%}else{%>
                                 
                                        <td><span class="badge rounded-pill alert-warning" style="color: rgb(255, 0, 0);"><%=orderData[i].status%></span></td>
                                    <%}%>
                                    <td><%=orderData[i].createdOn.toLocaleDateString()%></td>
                                    <td class="text-end">
                                        <a href="/admin/editUserorder?orderid=<%=orderData[i]._id%>" class="btn btn-md rounded font-sm btn-success">Edit</a>
                                        
                                        <!-- dropdown //end -->
                                    </td>
                                    <td>
                                        <%for(let j=0; j< orderData[i].Items.length; j++){%>
                                           <%if(orderData[i].return || orderData[i].Items[j].return ){%>
                                            <span>ðŸ”´</span>
                                           <%break%>
                                           <%}%>
                                        <%}%>
                                    </td>
                                   
                                </tr>
                               
                            <%}%>
                            </tbody>
                        </table>
                    </div> <!-- table-responsive //end -->
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
            <div class="pagination">
                <% if (currentPage > 1) { %>
                  <a href="/admin/userOrders?page=<%= currentPage - 1 %>">&laquo; Previous &nbsp;</a>
                <% } %>
              
                Page <%= currentPage %> of <%= totalPages %>
              <br>
                <% if (currentPage < totalPages) { %>
                  <a href="/admin/userOrders?page=<%= currentPage + 1 %>"> &nbsp; Next &raquo;</a>
                <% } %>
              </div>
    
              <br><br><br>
        </section> <!-- content-main end// -->
        <footer class="main-footer font-xs">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
                    <script>
                    document.write(new Date().getFullYear())
                    </script> Â©, Denim Stores  .
                </div>
                <div class="col-sm-6">
                    <div class="text-sm-end">
                        All rights reserved
                    </div>
                </div>
            </div>
        </footer>
    </main>
    <script src="admin-assets/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="admin-assets/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="admin-assets/js/vendors/select2.min.js"></script>
    <script src="admin-assets/js/vendors/perfect-scrollbar.js"></script>
    <script src="admin-assets/js/vendors/jquery.fullscreen.min.js"></script>
    <!-- Main Script -->
    <script src="admin-assets/js/main.js" type="text/javascript"></script>
</body>


